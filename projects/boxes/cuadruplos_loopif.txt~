box{
	vari #a = 1, #b = 2, #c, #i;
	vars #salida;
	vars #acum;

	mainbox(){
		if(#i > 105 or (#a <10)){
			#i = #a+20;		
		}else{
			#i = #a-10;
		}

		loopif( #b < 3 ){
			#a = #a + #b;
		}
		
		loop ( #c from 10 to 100 by -2 ) {
			#a = #a + 1;
		}

		say("adios");
		askuser("dame tu temperatura", #acum);
	}
}
